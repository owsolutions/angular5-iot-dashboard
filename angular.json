{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular5-iot-dashboard": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss"
              ]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "node_modules/@ng-dashboard/core/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/forms-style.css",
              "src/styles.scss",
              "node_modules/simplebar/dist/simplebar.css"
            ],
            "scripts": [
              "node_modules/simplebar/dist/simplebar.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "prodlocal": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prodlocal.ts"
                }
              ]
            },
            "prodmock": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prodmock.ts"
                }
              ]
            },
            "github": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.github.ts"
                }
              ]
            },
            "hmr": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hmr.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular5-iot-dashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular5-iot-dashboard:build:production"
            },
            "prodlocal": {
              "browserTarget": "angular5-iot-dashboard:build:prodlocal"
            },
            "prodmock": {
              "browserTarget": "angular5-iot-dashboard:build:prodmock"
            },
            "github": {
              "browserTarget": "angular5-iot-dashboard:build:github"
            },
            "hmr": {
              "browserTarget": "angular5-iot-dashboard:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular5-iot-dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "scripts": [
              "node_modules/simplebar/dist/simplebar.js"
            ],
            "styles": [
              "node_modules/@ng-dashboard/core/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/forms-style.css",
              "src/styles.scss",
              "node_modules/simplebar/dist/simplebar.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "angular5-iot-dashboard",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}